        "execa": "^8.0",
        "fs-extra": "^11",
        "giget": "^1.2.3",
        "lookpath": "^1.2.2",
        "open": "^10.1.0",
        "ora": "^8.0.1",
        "progress-stream": "^2.0",
        "semver": "^7.5",
        "tmp": "^0.2.3",
        "typanion": "^3.14.0",
