        "@wagmi/cli": "^2.1.4",
        "copyfiles": "^2",
        "eslint": "^8.57.0",
        "npm-run-all": "^4",
        "rimraf": "^5",
        "ts-node": "^10",
        "tsconfig": "workspace:*",
        "tslib": "^2",
        "typescript": "^5.4.3",
        "vitest": "^1.4.0"
