{
    "name": "@sunodo/cli",
    "version": "0.12.0",
    "description": "Sunodo CLI",
    "author": "Danilo Tuler <tuler@pobox.com>",
    "bin": {
        "sunodo": "./bin/run.js"
    },
    "type": "module",
    "homepage": "https://github.com/sunodo/sunodo",
    "license": "Apache-2.0",
    "exports": "./dist/index.js",
    "repository": "sunodo/sunodo",
    "files": [
        "/bin",
        "/dist",
        "/npm-shrinkwrap.json",
        "/oclif.manifest.json"
    ],
    "dependencies": {
        "@inquirer/confirm": "^3.1.1",
        "@inquirer/core": "^7.1.1",
        "@inquirer/input": "^2.1.1",
        "@inquirer/select": "^2.2.1",
        "@inquirer/type": "^1.2.1",
        "@oclif/core": "^3.26.0",
        "@oclif/plugin-help": "^6.0.20",
        "@oclif/plugin-plugins": "^5.0.1",
        "bytes": "^3.1",
        "chalk": "^5.3.0",
        "execa": "^8.0",
        "fs-extra": "^11",
        "lookpath": "^1.2.2",
        "open": "^10.1.0",
        "ora": "^8.0.1",
        "progress-stream": "^2.0",
        "semver": "^7.5",
        "tmp": "^0.2.3",
        "viem": "^2.9.2"
    },
    "devDependencies": {
        "@oclif/test": "^3.2.8",
        "@sunodo/contracts": "workspace:*",
        "@sunodo/devnet": "workspace:*",
        "@sunodo/eslint-config": "workspace:*",
        "@sunodo/token": "workspace:*",
        "@sunodo/wagmi-plugin-hardhat-deploy": "^0.3.0",
        "@types/bytes": "^3.1",
        "@types/fs-extra": "^11",
        "@types/inquirer": "^9",
        "@types/node": "^20.11.30",
        "@types/node-fetch": "^2.6",
        "@types/progress-stream": "^2.0",
        "@types/prompts": "^2.4",
        "@types/semver": "^7.5.8",
        "@types/tmp": "^0.2",
        "@wagmi/cli": "^2.1.4",
        "copyfiles": "^2",
        "eslint": "^8.57.0",
        "eslint-config-oclif": "^5.1.1",
        "eslint-config-oclif-typescript": "^3.1.3",
        "npm-run-all": "^4",
        "oclif": "^4.6.3",
        "rimraf": "^5",
        "ts-node": "^10",
        "tsconfig": "workspace:*",
        "tslib": "^2",
        "typescript": "^5.4.3",
        "vitest": "^1.4.0"
    },
    "oclif": {
        "bin": "sunodo",
        "dirname": "sunodo",
        "commands": "./dist/commands",
        "plugins": [
            "@oclif/plugin-help"
        ],
        "topicSeparator": " ",
        "macos": {
            "identifier": "io.sunodo.cli"
        }
    },
    "scripts": {
        "build": "run-s clean codegen compile copy-files",
        "clean": "rimraf dist",
        "codegen": "run-p codegen:wagmi",
        "codegen:wagmi": "wagmi generate",
        "compile": "tsc -b",
        "copy-files": "copyfiles -u 1 \"src/**/*.yaml\" \"src/**/*.env\" \"src/**/*.txt\" dist",
        "lint": "eslint \"src/**/*.ts*\"",
        "postpack": "rimraf oclif.manifest.json",
        "posttest": "pnpm lint",
        "prepack": "pnpm build && oclif manifest",
        "test": "vitest"
    },
    "engines": {
        "node": ">=18.0.0"
    },
    "bugs": "https://github.com/sunodo/sunodo/issues",
    "keywords": [
        "oclif"
    ],
    "types": "dist/index.d.ts"
}
