{
    "name": "@sunodo/cli",
    "version": "0.6.0",
    "description": "Sunodo CLI",
    "author": "Danilo Tuler <tuler@pobox.com>",
    "bin": {
        "sunodo": "./bin/run.js"
    },
    "type": "module",
    "homepage": "https://github.com/sunodo/sunodo",
    "license": "Apache-2.0",
    "exports": "./dist/index.js",
    "repository": "sunodo/sunodo",
    "files": [
        "/bin",
        "/dist",
        "/npm-shrinkwrap.json",
        "/oclif.manifest.json"
    ],
    "dependencies": {
        "@helia/car": "^1.0",
        "@helia/unixfs": "^1.4",
        "@inquirer/prompts": "^3.0",
        "@ledgerhq/hw-app-eth": "^6",
        "@ledgerhq/hw-transport-node-hid": "^6",
        "@metamask/sdk": "^0.5",
        "@oclif/core": "^2.9",
        "@oclif/plugin-help": "^5.2",
        "@oclif/plugin-plugins": "^3.1",
        "@oclif/plugin-update": "^3.1",
        "@urql/core": "^4.1",
        "@wagmi/chains": "^1.6",
        "@wagmi/core": "^1.3",
        "@walletconnect/ethereum-provider": "^2.9",
        "abitype": "^0.9",
        "ansi-colors": "^4",
        "bytes": "^3.1",
        "cli-progress": "^3.12",
        "execa": "^7.1",
        "fs-extra": "^11",
        "gauge": "^5.0",
        "giget": "^1.1",
        "graphql": "^16",
        "helia": "^1.3",
        "ipfs-car": "^1.0",
        "kubo-rpc-client": "^3.0",
        "lokijs": "^1.5",
        "node-fetch": "3.3",
        "open": "^9.0",
        "ora": "^7.0",
        "progress-stream": "^2.0",
        "qrcode-terminal": "^0.12",
        "tar-fs": "^3.0",
        "tmp": "^0.2",
        "viem": "^1.5",
        "web3-provider-ledger": "^1.0.6",
        "zlib": "^1.0"
    },
    "devDependencies": {
        "@graphql-codegen/cli": "^5",
        "@graphql-codegen/typed-document-node": "^5",
        "@graphql-codegen/typescript": "^4",
        "@graphql-codegen/typescript-operations": "^4",
        "@graphql-typed-document-node/core": "^3.2",
        "@oclif/test": "^2.3",
        "@sunodo/contracts": "*",
        "@sunodo/devnet": "*",
        "@sunodo/token": "*",
        "@sunodo/wagmi-plugin-hardhat-deploy": "*",
        "@types/bytes": "^3.1",
        "@types/fs-extra": "^11",
        "@types/gauge": "^5.0",
        "@types/inquirer": "^9",
        "@types/node": "^20",
        "@types/node-fetch": "^2.6",
        "@types/progress-stream": "^2.0",
        "@types/prompts": "^2.4",
        "@types/qrcode-terminal": "^0.12",
        "@types/tar-fs": "^2.0",
        "@types/tmp": "^0.2",
        "@wagmi/cli": "^1.3",
        "copyfiles": "^2",
        "eslint": "^8",
        "eslint-config-custom": "*",
        "eslint-config-oclif": "^4",
        "eslint-config-oclif-typescript": "^1",
        "npm-run-all": "^4",
        "oclif": "^3.9",
        "rimraf": "^5",
        "ts-node": "^10.9",
        "tsconfig": "*",
        "tslib": "^2",
        "typescript": "^5",
        "vitest": "^0.34"
    },
    "oclif": {
        "bin": "sunodo",
        "dirname": "sunodo",
        "commands": "./dist/commands",
        "plugins": [
            "@oclif/plugin-help",
            "@oclif/plugin-update"
        ],
        "topicSeparator": " ",
        "macos": {
            "identifier": "io.sunodo.cli"
        }
    },
    "scripts": {
        "build": "run-s clean codegen compile copy-files",
        "clean": "rimraf dist",
        "codegen": "run-p codegen:graphql codegen:wagmi",
        "codegen:graphql": "graphql-codegen",
        "codegen:wagmi": "wagmi generate",
        "compile": "tsc -b",
        "copy-files": "copyfiles -u 1 src/**/*.yml dist",
        "lint": "eslint . --ext .ts --config .eslintrc",
        "postpack": "rimraf oclif.manifest.json",
        "posttest": "yarn lint",
        "prepack": "yarn build && oclif manifest && oclif readme",
        "test": "vitest",
        "version": "run-s prepack"
    },
    "engines": {
        "node": ">=18.0.0"
    },
    "bugs": "https://github.com/sunodo/sunodo/issues",
    "keywords": [
        "oclif"
    ],
    "types": "dist/index.d.ts"
}
