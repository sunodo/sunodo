        "lint": "eslint \"src/**/*.ts*\"",
        "posttest": "pnpm lint",
        "prepack": "pnpm build",
        "test": "vitest"
    },
    "engines": {
        "node": ">=18.0.0"
    },
    "bugs": "https://github.com/sunodo/sunodo/issues",
    "keywords": [
        "cartesi",
        "cli"
    ],
    "types": "dist/index.d.ts"
