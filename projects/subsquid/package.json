{
    "name": "@sunodo/subsquid",
    "version": "0.6.0",
    "private": true,
    "scripts": {
        "build": "run-s tsc",
        "clean": "rimraf lib src/abi src/model",
        "codegen": "run-p codegen:database codegen:wagmi",
        "codegen:database": "squid-typeorm-codegen",
        "codegen:wagmi": "wagmi generate",
        "update": "npx npm-check-updates --filter /subsquid/ --upgrade && npm i -f",
        "db:migrate": "npx squid-typeorm-migration apply",
        "squid-etl:start": "node lib/processor.js",
        "squid-api:start": "squid-graphql-server",
        "tsc": "tsc"
    },
    "dependencies": {
        "@subsquid/archive-registry": "^3.1",
        "@subsquid/big-decimal": "^1.0",
        "@subsquid/evm-processor": "^1.7",
        "@subsquid/graphql-server": "^4.2",
        "@subsquid/typeorm-migration": "^1.2",
        "@subsquid/typeorm-store": "^1.2",
        "@wagmi/chains": "^1.6",
        "abitype": "^0.9",
        "dotenv": "^16",
        "ethers": "^6",
        "pg": "^8",
        "typeorm": "^0.3",
        "viem": "^1.4"
    },
    "devDependencies": {
        "@subsquid/typeorm-codegen": "^1.2",
        "@sunodo/wagmi-plugin-hardhat-deploy": "*",
        "@types/node": "^20",
        "@wagmi/chains": "^1.6",
        "@wagmi/cli": "^1.3",
        "npm-run-all": "^4",
        "rimraf": "^5",
        "synp": "^1",
        "typechain": "^8",
        "typescript": "^5"
    }
}
